{
    "manifest_version": 2,

    "name": "SpeedType",
    "description": "This extension converts code pieces into timed typing games",
    "version": "1.0",

    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ],

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["dist/speedtype-bundle.js"],
        "css": ["dist/styles.css"]
    }]
}
