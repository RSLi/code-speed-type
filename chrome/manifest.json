{
    "manifest_version": 2,

    "name": "SpeedType",
    "description": "This extension converts code pieces into timed typing games",
    "version": "1.0",

    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ],

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["js/jquery-3.2.0.min.js", "js/jquery-ui.min.js", "js/codemirror/lib/codemirror.js", "js/speedtype-chrome.js"],
        "css": ["js/codemirror/lib/codemirror.css"]
    }]
}
